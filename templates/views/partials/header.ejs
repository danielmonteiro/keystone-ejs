<div id="header">
	<div role="navigation" class="navbar navbar-default">
		<div class="container-fluid">
			<div class="navbar-header">
				<button type="button" data-toggle="collapse" data-target=".navbar-collapse" class="navbar-toggle">
					<span class="sr-only">Toggle navigation</span>
					<span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>
				</button>
				<a href="/" class="navbar-brand">foobar</a>
			</div>
			<div class="collapse navbar-collapse">
				<ul class="nav navbar-nav navbar-left">
					<% for( var i = 0;i < navLinks.length;i++ ){ %>

					<li class="<%= ( navLinks[i].key == section ? 'active' : null ) %>">
						<a href="<%= navLinks[i].href %>"><%= navLinks[i].label %></a>
					</li>

					<% } %>
				</ul>
				<ul class="nav navbar-nav navbar-right">
					<% if ( user ) { %>
					<% if ( user.canAccessKeystone ) { %>
					<li><a href="/keystone">Open Keystone</a></li>
					<% } %>
					<li><a href="/keystone/signout">Sign Out</a></li>
					<% } else { %>
					<li><a href="/keystone/signin">Sign In</a></li>
					<% } %>
				</ul>
			</div>
		</div>
	</div>
</div>
